Call Main()
Sub Main()
	Dim arrobjs, strobj
	
	arrobjs = rhino.getobjects("select objects to randomize colors")
	Randomize
	For Each strobj In arrobjs
		
		rhino.objectcolor strobj, RGB(Int(256 * Rnd), Int(256 * Rnd), Int(256 * Rnd))
		
	Next
	
End Sub